{
  "name": "cameronsmith/phalcon-php",
  "description": "The example project for phalcon php",
  "keywords": [
    "phalcon php",
    "phalcon",
    "cameronsmith"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Cameron Smith"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-pdo": "*",
    "ext-psr": "*",
    "ext-zip": "*",
    "ext-phalcon": "^4.0.0"
  },
  "repositories": [

  ],
  "support": {
    "source": "https://github.com/cameronsmith/PhalconPHP"
  },
  "autoload": {
    "psr-4": {
      "CameronSmith\\PhalconPHP\\": "app/",
      "CameronSmith\\PhalconPHP\\Test\\": "tests/"
    }
  },
  "scripts": {
    "phpcs": "docker exec -t phalcon-php-fpm ./vendor/bin/phpcs",
    "install": "docker exec -t phalcon-php-fpm composer install"
  },
  "version": "0.0.1",
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5",
    "phalcon/devtools": "~4.0"
  }
}
